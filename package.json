{
  "name": "dream-analyzer-bot",
  "version": "1.0.0",
  "description": "Telegram bot for dream analysis using AI",
  "main": "src/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
    "start": "npm run build && nodemon dist/index.js",
    "dev": "concurrently \"npm run build:watch\" \"npm run start\""
  },
  "dependencies": {
    "axios": "^1.6.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "redis": "^4.6.12",
    "telegraf": "^4.15.6"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "nodemon": "^3.0.2",
    "typescript": "^5.5.3",
    "@types/node": "^20.8.5",
    "@types/express": "^4.17.21"
  },
  "keywords": [
    "telegram",
    "bot",
    "dream",
    "analysis",
    "ai"
  ],
  "author": "",
  "license": "MIT"
}
